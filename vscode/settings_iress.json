{
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.lineHeight": 26,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "all",
    "editor.rulers": [80],
    "editor.snippetSuggestions": "top",
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "*.tsx": "typescriptreact",
        "*.scss": "scss"
    },
    "files.trimTrailingWhitespace": true,
    "search.exclude": {
        "**/.git": true,
        "**/out/": true,
        "**/build/": true,
        "**/javascript/dist": true,
        "*.orig": true
    },
    "sublimeTextKeymap.promptV3Features": true,
    "terminal.integrated.shell.windows": "C:\\cygwin64\\Cygwin.bat",
    "terminal.integrated.shellArgs.windows": ["/bin/xhere", "/bin/zsh"],
    "window.zoomLevel": 0,
    "workbench.colorTheme": "One Monokai Darker",
    "[scss]": {
        "editor.rulers": [80]
    },
    "[html]": {
        "editor.rulers": [128]
    },
    "[typescript]": {
        "editor.rulers": [100, 128]
    }
}